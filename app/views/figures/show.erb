<h1><%=@figure.name%></h1>

<br>

<h2>Titles</h2>
<%@figure.titles.each do |title|%>
  <h3><p><%=title.name%></p></h3>
<%end%>

<br>

<h2>Landmarks</h2>
<%@figure.landmarks.each do |landmark|%>
  <h3><p><a href="/landmarks/<%=landmark.id%>"><%=landmark.name%></a></p></h3>
<%end%>

<br>

<a href="/figures/<%=@figure.id%>/edit"> Edit Figure</a>
